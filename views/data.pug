doctype html
html
  head
    block head
      meta(charset='UTF-8')
      meta(name='viewport' content='width=device-width, initial-scale=1.0')
      title Top Films Form | #{title}

  body
    table
      tr
        th Film 
        th Names 
      each film in films
        tr
          td=film.name
          td=film.whoHasWatched


      